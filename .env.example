SPORTRADAR_API_KEY=your_key_here
SPORTRADAR_ACCESS_LEVEL=trial
SPORTRADAR_LANG=en
SPORTRADAR_FORMAT=json

# polling behavior
POLL_BASE_INTERVAL_MS=2000

